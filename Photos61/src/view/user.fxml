<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane 
		maxHeight="-Infinity" maxWidth="-Infinity" 
		minHeight="-Infinity" minWidth="-Infinity" 
		prefHeight="442.0" prefWidth="670.0" 
		xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1"
		fx:controller="view.UserController">
		
   <children>
      <ListView fx:id="listView" layoutX="31.0" layoutY="82.0" prefHeight="282.0" prefWidth="609.0" />
      <Button fx:id="addButton" layoutX="47.0" layoutY="378.0" mnemonicParsing="false" prefHeight="43.0" prefWidth="90.0" text="Add" 
      			onAction="#addAlbumEvent" />
      <Button fx:id="renameButton" layoutX="151.0" layoutY="378.0" mnemonicParsing="false" prefHeight="43.0" prefWidth="90.0" text="Rename" 
      		  onAction="#renameAlbumEvent" />
      <Button fx:id="deleteButton" layoutX="253.0" layoutY="378.0" mnemonicParsing="false" prefHeight="43.0" prefWidth="90.0" text="Delete" 
      			onAction="#deleteAlbumEvent" />
      <Button fx:id="logoutButton" layoutX="532.0" layoutY="378.0" mnemonicParsing="false" prefHeight="43.0" prefWidth="90.0" text="Logout" 
      		  onAction="#logoutEvent" />
      <Label layoutX="31.0" layoutY="14.0" prefHeight="60.0" prefWidth="240.0" text="Your Albums.">
         <font>
            <Font size="36.0" />
         </font>
      </Label>
      <Button fx:id="searchButton" layoutX="550.0" layoutY="22.0" mnemonicParsing="false" prefHeight="43.0" prefWidth="90.0" text="Search" 
      		  onAction="#searchEvent"/>
      <Button fx:id="openButton" layoutX="441.0" layoutY="22.0" mnemonicParsing="false" prefHeight="43.0" prefWidth="90.0" text="Open" 
      		  onAction="#openAlbumEvent"/>
   </children>
</AnchorPane>