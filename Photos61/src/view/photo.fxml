<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane 
			maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" 
			prefWidth="600.0" xmlns="http://javafx.com/javafx/15.0.1" 
			xmlns:fx="http://javafx.com/fxml/1"
			fx:controller="view.PhotoController">
   <children>
      <ImageView fx:id="thumbnail" fitHeight="298.0" fitWidth="341.0" layoutX="14.0" layoutY="29.0" pickOnBounds="true" preserveRatio="true" />
      <ListView fx:id="listView" layoutX="365.0" layoutY="180.0" prefHeight="147.0" prefWidth="222.0" />
      <Button fx:id="addTag" layoutX="27.0" layoutY="341.0" mnemonicParsing="false" prefHeight="36.0" prefWidth="66.0" text="Add Tag"
      			onAction="#addTagEvent" />
      <Button fx:id="logoutButton" layoutX="519.0" layoutY="341.0" mnemonicParsing="false" prefHeight="36.0" prefWidth="66.0" text="Logout"
      			onAction="#logoutEvent" />
      <Button fx:id="deleteTag" layoutX="105.0" layoutY="341.0" mnemonicParsing="false" prefHeight="36.0" prefWidth="87.0" text="Delete Tag"
      			onAction="#deleteTagEvent" />
      <Label fx:id="dateTimeCapture" layoutX="365.0" layoutY="45.0" prefHeight="36.0" prefWidth="185.0" text="Date-Time of Capture">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Label fx:id="caption" alignment="TOP_LEFT" layoutX="365.0" layoutY="91.0" prefHeight="67.0" prefWidth="199.0" text="Caption">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Button fx:id="recaption" layoutX="205.0" layoutY="341.0" mnemonicParsing="false" prefHeight="36.0" prefWidth="87.0" text="Recaption"
      			onAction="#recaptionEvent" />
      <Button fx:id="returnButton" layoutX="443.0" layoutY="341.0" mnemonicParsing="false" prefHeight="36.0" prefWidth="66.0" text="Return"
      			onAction="#returnEvent" />
   </children>
</AnchorPane>
